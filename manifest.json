{
  "manifest_version": 3,
  "name": "BelongsInWealthbox",
  "version": "1.0.0",
  "icons": {
    "16": "icons/Wealthbox-icon-RGB-1 Blue.png",
    "48": "icons/Wealthbox-icon-RGB-1 Blue.png",
    "128": "icons/Wealthbox-icon-RGB-1 Blue.png"
  },
  "description": "Automatically converts phone numbers in Wealthbox Contact records into clickable 'tel:' links.",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "*://*.crmworkspace.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.crmworkspace.com/*"
      ],
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ]
}