{
  "manifest_version": 3,
  "name": "BelongsInWealthbox",
  "version": "1.0.0",
  "icons": {
    "16": "icons/Wealthbox-icon-RGB-1 Blue.png",
    "48": "icons/Wealthbox-icon-RGB-1 Blue.png",
    "128": "icons/Wealthbox-icon-RGB-1 Blue.png"
  },
  "description": "Finally, a browswer extension that does what Wealthbox refuses to do: automatically converts Contact record phone numbers into clickable tel:// links.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.crmworkspace.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}